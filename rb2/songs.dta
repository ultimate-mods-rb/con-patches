                     (check_missed
                        {if {> {'+' [num_gems_miss] [num_gems_pass]} 0}
                           {if $fcring
                              {{{{get_track_panel} find bass} find streak_meter} star_deploy_stop}}}
                     )
                     (num_gems_hit 0)
                     (num_gems_combo 0)
                     (num_gems_miss 0)
