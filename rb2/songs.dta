                                    (1
                                       {set $pad_inst
                                          {switch $pad_inst
                                             (1 2)
                                             (2 3)
                                             3
                                          }
                                       }
                                    )
                                    (2
                                       {set $pad_map
                                          {switch $pad_map
                                             (1 2)
                                             (2 3)
                                             (3 4)
                                             (4 5)
                                             5
                                          }
                                       }
                                       {set_pad_map}
                                    )
                                    (3
                                       {set $cnt_slot1 {r_set_slot $cnt_slot1}}
                                    )
                                    (4
                                       {set $cnt_slot2 {r_set_slot $cnt_slot2}}
