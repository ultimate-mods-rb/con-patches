               {switch $var
                  (kPad_L2 kPad_L1)
                  (kPad_L1 kPad_R1)
                  (kPad_R1 kPad_R2)
                  (kPad_R2 kPad_X)
                  (kPad_X kPad_Circle)
                  (kPad_Circle kPad_Square)
                  (kPad_Square kPad_Tri)
                  (kPad_Tri kPad_DUp)
                  (kPad_DUp kPad_DDown)
